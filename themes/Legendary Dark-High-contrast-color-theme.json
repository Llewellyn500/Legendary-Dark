      "settings": {
        "foreground": "#36f9f6"
      }
    },
    {
      "name": "Elixir Functions",
      "scope": "source.elixir entity.name.function",
      "settings": {
        "foreground": "#72f1b8"
      }
    },
    {
      "name": "Elixir Constants",
      "scope": [
        "source.elixir constant.other.symbol.elixir",
