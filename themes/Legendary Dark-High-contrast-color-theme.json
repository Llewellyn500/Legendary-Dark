      "settings": {
        "foreground": "#36f9f6"
      }
    },
    {
      "name": "Elixir String Punctuation",
      "scope": "source.elixir punctuation.definition.string",
      "settings": {
        "foreground": "#72f1b8"
      }
    },
    {
      "name": "Elixir",
      "scope": [
        "source.elixir variable.other.readwrite.module.elixir",
