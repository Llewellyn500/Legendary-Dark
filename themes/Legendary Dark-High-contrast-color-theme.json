      "settings": {
        "foreground": "#ff7edb"
      }
    },
    {
      "name": "C placeholder",
      "scope": "constant.other.placeholder.c",
      "settings": {
        "foreground": "#72f1b8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "C preprocessors",
